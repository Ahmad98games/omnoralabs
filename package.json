{
  "name": "omnora-ecommerce-full",
  "version": "1.0.0",
  "description": "Full stack luxury e-commerce application",
  "scripts": {
    "install:all": "npm install && cd frontend && npm install && cd ../backend && npm install",
    "dev": "concurrently \"npm run server --prefix backend\" \"npm run dev --prefix frontend\"",
    "build": "npm run build --prefix frontend",
    "start": "npm run start --prefix backend",
    "server": "npm run start --prefix backend"
  },
  "devDependencies": {
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "concurrently": "^8.2.2"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "ngrok": "^5.0.0-beta.2",
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  }
}
